<UserControl x:Class="Xc.HiKVisionSdk.Isc.Wpf.Pages.Frs.BlackView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Xc.HiKVisionSdk.Isc.Wpf.Pages.Frs"
             mc:Ignorable="d" 
             xmlns:s="https://github.com/canton7/Stylet"
             d:DataContext="{d:DesignInstance local:BlackViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal" >
        <Button Command="{s:Action Black}" Margin="10,5">查询重点人员识别计划</Button>
        <Button Command="{s:Action BlackSearch}" Margin="10,5">查询重点人员识别记录</Button>
    </StackPanel>
</UserControl>
